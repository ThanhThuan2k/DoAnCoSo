@{
	ViewData["Title"] = "DanhSachHangSanXuat";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section css{ 
	<link href="~/Admin/css/NotificationModal.css" rel="stylesheet" />
}

<div class="card-box pb-10">
	<div class="h5 pd-20 mb-0"><span>Hãng sản xuất</span></div>
	<table class="data-table table nowrap">
		<thead>
			<tr>
				<th>Id</th>
				<th class="table-plus">Thương Hiệu</th>
				<th>Tên hãng</th>
				<th class="datatable-nosort">Actions</th>
			</tr>
		</thead>
		<tbody class="tbody">
		</tbody>
	</table>
</div>

<div class="notification-modal d-none">
	Không có dữ liệu
</div>

@section js{
	<script src="~/Admin/js/HangSanXuat/HangSanXuat.js"></script>
}