
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section css{ 
	<link href="~/Admin/css/NotificationModal.css" rel="stylesheet" />
}

<div class="card-box pb-10">
	<div class="h5 pd-20 mb-0 d-flex justify-content-between">
		<span>Danh sách nhân sự</span>
		<button class="btn btn-success create-btn" asp-action="ThemMoi" asp-controller="Admin" asp-area="Admin">Thêm mới</button>
	</div>
	<table class="data-table table nowrap">
		<thead>
			<tr>
				<th>Id</th>
				<th class="table-plus">Avatar</th>
				<th>Tên hiển thị</th>
				<th class="datatable-nosort">Hoạt động lần cuối</th>
				<th class="datatable-nosort">Action</th>
			</tr>
		</thead>
		<tbody class="tbody">
			
		</tbody>
	</table>
</div>

<div class="notification-modal d-none mt-2">
	Không có dữ liệu
</div>

@section js{
	<script src="~/Admin/js/Admin/DanhSach.js"></script>
}